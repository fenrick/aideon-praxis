{
  "name": "@aideon/praxis-design-system",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "description": "Aideon Praxis shared Svelte components, theming, and design tokens",
  "author": {
    "name": "Aideon Praxis contributors",
    "email": "201621+fenrick@users.noreply.github.com"
  },
  "exports": {
    ".": "./src/index.ts",
    "./ui/*": "./src/ui/*",
    "./styles/*": "./src/styles/*",
    "./theme/*": "./src/theme/*"
  },
  "svelte": "./src/index.ts",
  "types": "./src/index.ts",
  "typesVersions": {
    "*": {
      "ui/*": [
        "src/ui/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "theme/*": [
        "src/theme/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,js,svelte}\"",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "@fluentui/web-components": "^2.6.1",
    "@iconify/svelte": "^5.0.2",
    "svelte": "^5.42.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3"
  }
}
