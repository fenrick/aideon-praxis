{
  "name": "@aideon/app",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "license": "MIT",
  "type": "commonjs",
  "description": "Aideon Praxis Electron host",
  "author": "Aideon Praxis contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/<owner>/<repo>.git"
  },
  "scripts": {
    "dev": "concurrently -k \"vite build --watch\" \"tsup --watch\" \"wait-on dist/renderer/index.html && electron .\"",
    "build": "tsup && vite build",
    "dist": "yarn build && electron-builder -c electron-builder.yml",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "electron": "31.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "tsup": "^8.0.0",
    "electron-builder": "^24.13.3",
    "concurrently": "^9.0.0",
    "wait-on": "^7.2.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.4.0"
  }
}
