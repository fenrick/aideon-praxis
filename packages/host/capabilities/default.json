{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["splash", "main", "settings", "about", "status"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:window:default",
    "core:webview:default",
    "core:event:default",
    "log:default",
    "opener:default",
    { "identifier": "shell:default" },
    "shell:allow-spawn",
    "shell:allow-execute"
  ]
}
